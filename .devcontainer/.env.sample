# 環境情報
# 語群はci dev stg prod
APP=dev
IS_HIDDEN_SQL=false
IS_HIDDEN_TEST_SQL=false

# Gメール秘密鍵配置場所
CLIENT_SECRET_PATH=/data/client_secret.json
GMAIL_PORT=5555
OPENAI_API_KEY=yourToken

# Gメール取得ラベル
LABEL=営業/案件

# DB周り
MYSQL_USER=user
MYSQL_PASSWORD=password
DB_HOST=mysql # DB_HOSTにはDBコンテナ名を入れる。
DB_PORT=3306
MYSQL_DATABASE=development
MYSQL_TEST_DATABASE=test

# Google OAuth2設定
GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your_google_client_secret

# JWT設定
JWT_SECRET_KEY=your_jwt_secret_key_here

# アプリケーション設定
APP_NAME=Gmail Auth App
LOG_LEVEL=info
GO_PORT=8080
REMOTE_CONTAINER_PORT=5994 # この値を変える場合はDockerfileのportも変える必要がある。
GO_DEBUG_PORT=2345 # この値を変える場合はair.tomlのfull_binのlistenも変える必要がある。


# フロントエンド設定（既存のWebアプリケーション用）
USE_SSL=FALSE
FRONT_DOMAIN=http://localhost:3000
FRONT_SSL_DOMAIN=https://localhost:3000
